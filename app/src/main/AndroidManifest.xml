<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tnwls.my_application">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".List_Local" />
        <activity android:name=".List_Local_In" />
        <activity android:name=".List_Thema" />
        <activity android:name=".List_Thema_In" />
        <activity android:name=".Festival_In" />
        <activity android:name=".Festival_In_Introduce" />
        <activity android:name=".Festival_In_OperationGuide" />
        <activity android:name=".Festival_In_Facilities" />
        <activity android:name=".BookMark"/>

    </application>

</manifest>